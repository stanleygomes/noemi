<idea-plugin>
    <id>com.nazarethlabs.noemi</id>
    <name>Noemi Theme</name>
    <vendor>stanleygomes</vendor>

    <!-- Plugin compatibility -->
    <idea-version since-build="243"/>

    <depends>com.intellij.modules.platform</depends>

    <!-- Theme declarations -->
    <extensions defaultExtensionNs="com.intellij">
      <themeProvider id="com.nazarethlabs.noemi.july" path="/themes/Noemi_July.theme.json" />
      <themeProvider id="com.nazarethlabs.noemi.november" path="/themes/Noemi_November.theme.json" />

      <bundledColorScheme path="/themes/Noemi_July.xml"/>
      <bundledColorScheme path="/themes/Noemi_November.xml"/>
    </extensions>
</idea-plugin>
